{
  "name": "@netlify/sdk",
  "version": "1.39.0",
  "description": "",
  "type": "module",
  "bin": {
    "ntli": "./dist/bin/index.js",
    "netlify-integration": "./dist/bin/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/api/netlify.d.ts",
      "default": "./dist/api/netlify.js"
    },
    "./bin": {
      "default": "./dist/bin/index.js"
    },
    "./commands": {
      "default": "./dist/cli/commands/index.js"
    },
    "./cli-utils": {
      "default": "./dist/cli/util.js"
    },
    "./model": {
      "types": "./dist/integration/connect/model/index.d.ts",
      "default": "./dist/integration/connect/model/index.js"
    },
    "./connector": {
      "types": "./dist/integration/connect/types/index.d.ts",
      "default": "./dist/integration/connect/types/index.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/stitch": "^9.0.3",
    "@graphql-tools/utils": "^10.1.2",
    "@graphql-tools/wrap": "^10.0.1",
    "@netlify/functions": "^2.6.0",
    "@netlify/integrations": "^0.6.0",
    "@stackbit/dev": "^1.0.19",
    "camelcase": "^8.0.0",
    "chalk": "^5.2.0",
    "commander": "^11.1.0",
    "crypto-js": "^4.2.0",
    "esbuild": "^0.20.0",
    "esbuild-plugin-copy": "^2.1.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.0",
    "graphql-scalars": "^1.22.5",
    "js-xxhash": "^3.0.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "open": "^9.1.0",
    "outdent": "^0.8.0",
    "safe-stable-stringify": "^2.4.3",
    "toml": "^3.0.0",
    "typescript": "^5.4.3",
    "wretch": "^2.7.0",
    "zod": "^3.22.4",
    "zod-metadata": "^1.1.0",
    "zod-to-json-schema": "^3.21.4",
