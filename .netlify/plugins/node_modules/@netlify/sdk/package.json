  "devDependencies": {
    "@netlify/build": "^29.39.1",
    "@stackbit/types": "^0.11.7",
    "@tsconfig/node18": "^18.2.4",
    "@tsconfig/recommended": "^1.0.5",
    "@types/chance": "^1.1.5",
    "@types/crypto-js": "^4.1.3",
    "@types/fs-extra": "^11.0.3",
    "@types/js-yaml": "^4.0.8",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "4.14.202",
    "@types/node": "^18.18.12",
    "@types/uglify-js": "^3.17.3",
    "@types/uuid": "^9.0.0",
    "chance": "^1.1.11",
    "graphql": "^16.8.1",
    "publint": "^0.2.5",
    "uuid": "^9.0.0",
    "vitest": "^1.0.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "prebuild": "./scripts/bundle-version.js",
    "build": "pnpm prebuild && tsc --project tsconfig.build.json",
    "watch": "tsc --project tsconfig.build.json --watch",
    "lint": "publint",
    "demo": "cd demo && ([ ! -d '../dist' ] || [ ! -d '../node_modules' ] || [ ! -d './node_modules' ]) && pnpm run build:setup || pnpm run build:w-ntli",
    "test": "pnpm run build && vitest"
